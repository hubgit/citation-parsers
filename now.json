{
  "version": 2,
  "name": "my-react-app",
  "alias": "my-react-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/js/$1"
    },
    { "src": "/(.*)\\.(.*)", "dest": "/$1.$2" },
    { "src": "/sockjs-node/(.*)", "dest": "/sockjs-node/$1" },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/index.html"
    }
  ]
}
